{
  "name": "visual-product-comparison-engine",
  "description": "Visual Product Comparison Engine - Offline Visual Search Intelligence",
  "private": true,
  "version": "0.0.1",
  "main": "dist-electron/main.js",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "download-model": "tsx ml/scripts/download_model.ts",
    "generate-embeddings": "tsx ml/scripts/generateEmbeddings.ts",
    "generate-thumbnails": "tsx ml/scripts/buildIndex.ts",
    "postinstall": "electron-rebuild -f",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "lint": "eslint ."
  },
  "build": {
    "appId": "com.vpce.app",
    "productName": "Visual Product Comparison Engine",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "dist-electron/**/*"
    ],
    "extraResources": [
      {
        "from": "ml/models/resnet50",
        "to": "ml/models/resnet50"
      },
      {
        "from": "datazip/embeddings.hnsw.zip",
        "to": "data/embeddings.hnsw.zip"
      },
      {
        "from": "datazip/products.db.zip",
        "to": "data/products.db.zip"
      },
      {
        "from": "datazip/thumbnails.zip",
        "to": "data/thumbnails.zip"
      }
    ],
    "mac": {
      "target": "dmg",
      "category": "public.app-category.utilities"
    },
    "dmg": {
      "title": "Visual Product Comparison Engine"
    }
  },
  "dependencies": {
    "@tensorflow/tfjs-node": "^4.22.0",
    "adm-zip": "^0.5.16",
    "better-sqlite3": "^12.6.2",
    "framer-motion": "^12.34.3",
    "hnswlib-node": "^3.0.0",
    "sharp": "^0.34.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@tailwindcss/vite": "^4.2.1",
    "@types/adm-zip": "^0.5.7",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "concurrently": "^9.2.1",
    "electron": "^40.6.1",
    "electron-builder": "^26.8.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "lucide-react": "^0.575.0",
    "radix-ui": "^1.4.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "shadcn": "^3.8.5",
    "tailwind-merge": "^3.5.0",
    "tailwindcss": "^4.2.1",
    "tsx": "^4.21.0",
    "tw-animate-css": "^1.4.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.48.0",
    "vite": "^7.3.1",
    "vite-plugin-electron": "^0.29.0",
    "wait-on": "^9.0.4"
  }
}